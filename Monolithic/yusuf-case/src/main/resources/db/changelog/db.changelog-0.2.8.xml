<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet author="tevfikuykun" id="1" >

        <createTable tableName="city_counties">
            <column name="city_id" type="${uuid_type}"/>
            <column name="counties_id" type="${uuid_type}"/>
        </createTable>

        <createTable tableName="county_streets">
            <column name="county_id" type="${uuid_type}"/>
            <column name="streets_id" type="${uuid_type}"/>
        </createTable>


        <rollback>
            <dropTable tableName="city_counties"/>
            <dropTable tableName="county_streets"/>
        </rollback>


    </changeSet>

</databaseChangeLog>