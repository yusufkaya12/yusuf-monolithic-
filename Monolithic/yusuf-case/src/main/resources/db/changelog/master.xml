  <property name="timestamp_type" value="timestamp with time zone" dbms="h2, oracle"/>
  <property name="timestamp_type" value="datetimeoffset" dbms="mssql"/>
  <property name="timestamp_type" value="timestamp" dbms="mysql"/>
  <property name="timestamp_type" value="timestamptz" dbms="postgresql"/>
  <property name="uuid_type" value="uuid" dbms="h2, postgresql"/>
